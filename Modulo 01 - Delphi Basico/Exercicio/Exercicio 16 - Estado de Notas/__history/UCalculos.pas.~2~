unit UCalculos;

interface
  function CalcularMedia(const aNota01, aNota02, aNota03: Double): Double;
implementation

  function CalcularMedia(const aNota01, aNota02, aNota03: Double): Double;
    var
      xMedia: Double;
    begin
      xMedia := (aNota01 + aNota02 + aNota03) / 3;
      Result := xMedia;
    end;
end.
